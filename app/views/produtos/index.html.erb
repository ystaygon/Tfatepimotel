<p id="notice"><%= notice %></p>

<h1>Listing Produtos</h1>

<table>
  <thead>
    <tr>
      <th>Pro id</th>
      <th>Pro noproduto</th>
      <th>Pro nrquantidade</th>
      <th>Pro vlcompra</th>
      <th>Pro vlvenda</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produtos.each do |produto| %>
      <tr>
        <td><%= produto.Pro_Id %></td>
        <td><%= produto.Pro_Noproduto %></td>
        <td><%= produto.Pro_Nrquantidade %></td>
        <td><%= produto.Pro_Vlcompra %></td>
        <td><%= produto.Pro_Vlvenda %></td>
        <td><%= link_to 'Show', produto %></td>
        <td><%= link_to 'Edit', edit_produto_path(produto) %></td>
        <td><%= link_to 'Destroy', produto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produto', new_produto_path %>
